<section class="header">
  <section class="header-container">
      <div class="header-component-left">
        <a href="/"><img src='https://www.trzcacak.rs/myfile/full/168-1685213_marijuana-order-online-marijuana-buds.png' alt="Logo"/></a>
      </div>
      <div class="header-component-right">
        <a href="/user">
          <button>Profile</button>
        </a>
        <p>Level: {{article.level}}</p>
      </div>
</section>
</section>
<div class="container">
    <div class="content">
        <h1 class="article-title">{{article.title}}</h1>
       <div class="article-container">

        {{#each article.q}}
            <h3 class="article-header">{{this.q}}</h3>
            <h5 class="article-sub">{{this.a}}</h5>
        {{/each}}
       </div>

        {{#if previouseArticle}}
            <a href="/article/{{article.level}}/{{previouseArticle}}"><button class="start">Previouse article</button></a>
        {{/if}}
        {{#if nextArticle}}
            <a href="/article/{{article.level}}/{{nextArticle}}"><button class="start">Next article</button></a>
        {{/if}}
    </div>
</div>
