<section class="header">
  <section class="header-container">
      <div class="header-component-left">
        <a href="/"><img src='https://www.trzcacak.rs/myfile/full/168-1685213_marijuana-order-online-marijuana-buds.png' alt="Logo"/></a>
      </div>
      <div class="header-component-right">
        <a href="/user">
          <button>Profile</button>
        </a>
        <p>Level: {{quiz.level}}</p>
      </div>
</section>
</section>
<div class="container">
    <div class="content">
        <h1 class="article-title">To unlock the next curiculem you have to pass this test</h1>
       <div class="article-container">
         <form action="/test/{{quiz.level}}/check" method="GET">
          {{#each quiz.questions}}
            <br><label>{{this.q}}</label><br>
            {{#if this.tick}}

            {{#with possible}}
            {{#each this}}
              <input type="checkbox" name='{{this.correct}}' value='{{this.a}}'>{{this.a}}<br>
            {{/each}}
            {{/with}}
            {{else}}
            <input type="text" name='{{this.a}}' value=""> <br>
            {{/if}}
          {{/each}}
        
        <button type="submit" class="start">Finish Curiculem</button>
        </form>
       </div>
    </div>
</div>


{{log this}}